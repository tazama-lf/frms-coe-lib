{
  "name": "@tazama-lf/frms-coe-lib",
  "version": "6.0.0-rc.12",
  "description": "FRMS Center of Excellence package library",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tazama-lf/frms-coe-lib.git"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "@tazama-lf:registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "scripts": {
    "build": "npx rimraf lib && tsc --project tsconfig.json && copyfiles -f src/helpers/proto/*.proto lib/helpers/proto",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "clean": "npx rimraf lib node_modules coverage package-lock.json",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint",
    "lint:eslint-unused": "eslint --report-unused-disable-directives",
    "lint:eslint-no-inline": "eslint --no-inline-config",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "dry": "npm publish --dry-run",
    "prepare": "husky"
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.5.0",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "arangojs": "^8.8.1",
    "dotenv": "^17.2.3",
    "elastic-apm-node": "^4.14.0",
    "ioredis": "5.7.0",
    "node-cache": "^5.1.2",
    "pino": "^10.0.0",
    "pino-elasticsearch": "^8.1.0",
    "protobufjs": "^7.5.4",
    "uuid": "^11.1.0"
  },
  "keywords": [],
  "author": "Tazama.org",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/jest": "^30.0.0",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.36.0",
    "eslint-config-love": "^121.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "ioredis-mock": "8.9.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
